{
    "name": "@bsasolutions/pkg_coreui",
    "version": "0.1.3",
    "type": "module",
    "main": "dist/index.umd.js",
    "module": "dist/index.es.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./styles": "./dist/style.css"
    },
    "files": [
        "dist"
    ],
    "exclude": [
        "playground"
    ],
    "scripts": {
        "build:ts": "vue-tsc -b",
        "build": "npm run build:ts && vite build",
        "dev": "vite playground --config playground/vite.config.ts",
        "build-pg": "vite build playground --config playground/vite.config.ts",
        "preview": "vite preview playground --config playground/vite.config.ts"
    },
    "peerDependencies": {
        "lucide-vue-next": "^0.553.0",
        "primevue": "^3.50.0",
        "vue": "^3.5.0",
        "vue-router": "^4.3.0",
        "vue-toastification": "^2.0.0-rc.5"
    },
    "devDependencies": {
        "@tailwindcss/postcss": "^4.1.16",
        "@tailwindcss/vite": "^4.1.16",
        "@types/node": "^24.6.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/runtime-dom": "^3.5.22",
        "@vue/tsconfig": "^0.8.1",
        "autoprefixer": "^10.4.21",
        "postcss": "^8.5.6",
        "prettier": "^3.6.2",
        "sass": "^1.69.5",
        "tailwindcss": "^4.0.0",
        "typescript": "~5.9.3",
        "vite": "^7.1.7",
        "vite-plugin-dts": "^4.5.4",
        "vitest": "^3.2.4",
        "vue-tsc": "^3.1.0"
    },
    "license": "MIT",
    "keywords": [
        "vue",
        "typescript",
        "ui",
        "crud",
        "registry",
        "components",
        "kit"
    ]
}
